                                                                            <span id="z1" class="active"></span>
                                                                            <span id="z2" class="active"></span>
                                                                            <span id="z3"></span>
                                                                            <span id="z4"></span>
                                                                            <span id="z5"></span>
                                                                        </div>


                                                                        <% }else if(item.reit <=3){%>
