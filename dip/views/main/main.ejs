        <div class="swiper-container">
            <img class="pictarr slick-arrow.slick-prev" id="my-arrow-prev" src="/pict/left.png">
            <div class="swiper-wrapper">

                <% for (let item of shmot) {%>
                    <div class="slide">
                        <% for (let arr of artist) {%>
